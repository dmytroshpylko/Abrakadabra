var a = [17, 1, 3, 0, 4, 9];
var b = [1, 3, 4, 5, 6, 7];

function search(a, b) {
  var result = [];
  for(var i = 0; i < a.length; i++) {
    if (b.indexOf(a[i]) == -1) {
      result.push(a[i]);
    }
  }
  return result;
}

console.log(search(b, a));






